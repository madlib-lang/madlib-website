import type { Action } from "MadUI"
import type { State } from "@client/State"

copyToClipboardHandler :: String -> Action State
export copyToClipboardHandler = (c) => (s, _) => {
  return [
    Wish((_, good) => good((_) => {
      #- navigator.clipboard.writeText(c) -#

      return {
        ...s,
        playground: {
          ...s.playground,
          data: {
            ...s.playground.data,
            code: c
          }
        }
      }
    }))
  ]
}

